{
    "manifest_version": 2,
    "name": "EntityCardsCounter",
    "version": "1.0",
    "description": "Counts the entity cards appearing in Google SERPs.",
    "icons": {
        "48": "icons/icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.google.com/search*",
                "*://*.duckduckgo.com/?q=*"
            ],
            "js": [
                "counter.js"
            ]
        }
    ]
}